{
  "name": "slickgrid2",
  "description": "SlickGrid with pinning, more customizability & faster column sizing",
  "version": "15.0.9",
  "license": "MIT",
  "homepage": "https://github.com/coatue-oss/slickgrid2",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/coatue-oss/slickgrid2.git"
  },
  "contributors": [
    "Michael Leibman",
    "Eric Miller",
    "Albert Wu",
    "Boris Cherny <boris@performancejs.com>",
    "Chris Khoo <chris.khoo@gmail.com>"
  ],
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "scripts": {
    "build": "npm-run-all -s clean build:sass build:compat build:commonjs",
    "build:compat": "rollup -c rollup.js",
    "build:commonjs": "tsc -d -m commonjs",
    "build:sass": "node-sass src/slick.grid.sass dist/slick.grid.css",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build",
    "watch": "npm-run-all -pr watch:*",
    "watch:compat": "rollup -c rollup.js --watch",
    "watch:commonjs": "tsc -m commonjs -w",
    "watch:sass": "node-sass -w src/slick.grid.sass dist/slick.grid.css"
  },
  "dependencies": {
    "@bcherny/jquery.threedubmedia": "^1.0.0",
    "antiscroll": "^1.0.0",
    "jquery": "^3.2.1",
    "lodash": "^4.17.4"
  },
  "devDependencies": {
    "@types/jquery": "^2.0.41",
    "@types/lodash": "^4.14.65",
    "jquery-sparkline": "^2.4.0",
    "jquery-ui-dist": "^1.12.1",
    "node-sass": "^4.5.2",
    "npm-run-all": "^4.0.2",
    "rollup": "^0.43.0",
    "rollup-plugin-typescript": "^0.8.1",
    "rollup-watch": "^4.0.0",
    "tslint": "^5.1.0",
    "typescript": "^2.2.2"
  },
  "keywords": [
    "slickgrid",
    "grid",
    "table",
    "fast",
    "virtual",
    "pinned",
    "performance",
    "autosize",
    "column",
    "size"
  ]
}
