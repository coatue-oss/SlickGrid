{
  "name": "slickgrid2",
  "description": "SlickGrid with pinning, more customizability & faster column sizing",
  "version": "15.0.6",
  "license": "MIT",
  "homepage": "https://github.com/coatue-oss/SlickGrid",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/coatue-oss/SlickGrid.git"
  },
  "contributors": [
    "Michael Leibman",
    "Eric Miller",
    "Albert Wu",
    "Boris Cherny <boris@performancejs.com>",
    "Chris Khoo"
  ],
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "scripts": {
    "build:compat": "rollup -c rollup.js",
    "build:commonjs": "tsc -d -m commonjs",
    "build:sass": "node-sass src/slick.grid.sass dist/slick.grid.css",
    "build": "npm run clean && npm run build:sass && npm run build:compat && npm run build:commonjs",
    "clean": "rm -rf dist; exit 0",
    "watch:compat": "rollup -c rollup.js --watch",
    "watch:commonjs": "tsc -m commonjs -w",
    "watch:sass": "node-sass -w src/slick.grid.sass dist/slick.grid.css",
    "watch": "npm run watch:compat & npm run watch:sass & npm run watch:commonjs",
    "prepublish": "npm run build; exit 0"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.65",
    "jquery-simulate": "github:jquery/jquery-simulate#v1.0.1",
    "jquery-sparkline": "^2.4.0",
    "jquery-ui-dist": "^1.12.1",
    "node-sass": "^4.5.2",
    "rollup": "^0.43.0",
    "rollup-plugin-typescript": "^0.8.1",
    "rollup-watch": "^3.2.2",
    "tslint": "^5.1.0",
    "typescript": "^2.2.2"
  },
  "dependencies": {
    "@bcherny/jquery.threedubmedia": "^1.0.0",
    "@types/jquery": "^2.0.41",
    "antiscroll": "^1.0.0",
    "jquery": "^3.2.1",
    "lodash": "^4.17.4"
  },
  "keywords": [
    "slickgrid",
    "grid",
    "table",
    "fast",
    "virtual",
    "pinned",
    "performance",
    "autosize",
    "column",
    "size"
  ]
}
