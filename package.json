{
  "name": "slickgrid2",
  "description": "SlickGrid with pinning, more customizability & faster column sizing",
  "version": "12.0.6",
  "license": "MIT",
  "homepage": "https://github.com/coatue/SlickGrid",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/coatue/SlickGrid.git"
  },
  "contributors": [
    "Michael Leibman",
    "Eric Miller",
    "Albert Wu",
    "Boris Cherny <boris@performancejs.com>",
    "Chris Khoo"
  ],
  "main": "./slick.grid.js",
  "scripts": {
    "build": "rollup -c rollup.main.js && rollup -c rollup.compat.js",
    "install": "napa && cd node_modules/jquery.sparkline && make",
    "watch": "rollup -c rollup.main.js --watch & rollup -c rollup.compat.js --watch",
    "prepublish": "npm run build"
  },
  "napa": {
    "jquery.sparkline": "gwatts/jquery.sparkline#v2.1.2",
    "jquery.threedubmedia": "threedubmedia/jquery.threedubmedia#2.2"
  },
  "devDependencies": {
    "jquery-simulate": "github:jquery/jquery-simulate#v1.0.1",
    "jquery-ui-dist": "^1.12.1",
    "qunit": "^0.9.1",
    "rollup": "^0.36",
    "rollup-plugin-typescript": "^0.8.1",
    "rollup-watch": "^2.5.0",
    "tslint": "^4.0.2",
    "typescript": "^2.0.10"
  },
  "dependencies": {
    "@types/jquery": "^2.0.34",
    "antiscroll": "^1.0.0",
    "jquery": "^3.1.1",
    "napa": "^2.3.0"
  },
  "keywords": [
    "slickgrid",
    "grid",
    "table",
    "fast",
    "virtual",
    "pinned",
    "performance",
    "autosize",
    "column",
    "size"
  ]
}
